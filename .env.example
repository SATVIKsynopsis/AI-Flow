# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file with real secrets!

# Supabase Configuration (Required for authentication)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Calendar API (Required for calendar integration)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Google Gemini AI API (Required for AI scheduling features)
VITE_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Additional configuration
VITE_APP_NAME=AI-Flow
VITE_APP_VERSION=1.0.0